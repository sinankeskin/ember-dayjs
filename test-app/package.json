{
  "name": "test-app",
  "version": "0.0.0",
  "description": "day.js template helpers and a service for Ember.",
  "keywords": [
    "dayjs",
    "ember-dayjs",
    "date helpers"
  ],
  "repository": "https://github.com/sinankeskin/ember-dayjs",
  "license": "MIT",
  "author": "Sinan Keskin <sinan@kesk.in> (https://kesk.in)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "deploy": "npm version patch && git push && git push origin --tags && npm publish",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "update": "ecu --to latest && ncu -u && npm install && npm run lint:fix && npm run lint && git commit -am \"Update some packages\" && git push",
    "update:all": "run-s update deploy"
  },
  "dependencies": {
    "ember-dayjs": "*"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-decorators": "^7.20.13",
    "@ember/optional-features": "^2.0.0",
    "@ember/string": "^3.0.1",
    "@ember/test-helpers": "^2.9.3",
    "@embroider/test-setup": "^2.1.1",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^7.6.0",
    "ember-auto-import": "^2.6.0",
    "ember-cli": "~4.10.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-load-initializers": "^2.1.2",
    "ember-page-title": "^7.0.0",
    "ember-qunit": "^6.1.1",
    "ember-resolver": "^10.0.0",
    "ember-source": "^4.10.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^5.3.3",
    "ember-try": "^3.0.0-beta.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-ember": "^11.9.0",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "loader.js": "^4.7.0",
    "prettier": "^2.8.3",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "webpack": "^5.75.0"
  },
  "peerDependencies": {
    "ember-source": "^3.28.0 || ^4.0.0"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  },
  "private": true
}
