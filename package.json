{
  "name": "ember-dayjs",
  "version": "0.1.23",
  "description": "day.js template helpers and a service for Ember.",
  "keywords": [
    "ember-addon",
    "dayjs",
    "ember-dayjs",
    "date helpers"
  ],
  "repository": "https://github.com/sinankeskin/ember-dayjs",
  "license": "MIT",
  "author": "Sinan Keskin <sinan@kesk.in> (https://kesk.in)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel 'lint:!(fix)'",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "deploy": "npm version patch && git push && git push origin --tags && npm publish",
    "test": "npm-run-all lint test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "update": "ecu --to latest && ncu -u && npm install && npm run lint:fix && npm run lint && git commit -am 'Update some packages' && git push",
    "update:all": "run-s update deploy"
  },
  "dependencies": {
    "dayjs": "^1.10.5",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^5.7.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.2.8",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.0.1",
    "ember-cli": "~3.26.1",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-page-title": "^6.2.2",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "^3.27.5",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.5.0",
    "ember-try": "^1.4.0",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.5.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.1",
    "qunit": "^2.16.0",
    "qunit-dom": "^1.6.0",
    "webpack": "^5.39.1"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
